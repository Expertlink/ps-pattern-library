{{#with review}}
<div class="reviews-summary">
  <span class="review-stars-average stars stars-4-5">{{ average list "rating" }} stars</span>
  <span class="ratings-total"> ({{ count list }} Ratings)</span><br />
  <strong>{{ count list }}</strong> <span class="hidden-xs">Psychic Source member</span> reviews since <strong>Mar 2012</strong>
</div>
{{/with}}
