{{#with review}}
<div class="reviews-summary">
  <p class="review-stars-average"><span class="stars stars-4-5">{{ average list "rating" }} stars</span><span class="ratings-total"> ({{ count list }} Ratings)</span></p>
  <p><strong>{{ count list }}</strong> <span class="hidden-xs">Psychic Source member</span> reviews since <strong>Mar 2012</strong></p>
</div>
{{/with}}
