---
status: inprogress
---

{{> pages/_header }}

<div class="container-page">
  <div class="box-full box-default-lg box-lighter box-sheer">
    <div class="hidden-md hidden-lg">
      {{> toolbar/toolbar }}
    </div>
    <div class="container-fluid prairie-dog-xs prairie-dog-sm no-gutters-xs no-gutters-sm" id="our-psychics-container">
      <div class="col-md-3" id="our-psychics-sidebar">
        <div class="prairie-dog-dialog box box-lighter box-sheer box-shadow u-margin-ends" id="our-psychics-filters" data-parent="#our-psychics-container">
          <h4 class="u-no-margin u-padding hidden-xs hidden-sm">Filter by</h4>
          {{> filters/filter-menu }}
          <div class="text-center u-padding text-nowrap">
            <button class="btn btn-link" type="button" data-dismiss="prairie-dog"><span class="text-danger">Clear All</span></button>
            <button class="btn btn-success btn-shaded js-apply-button" type="button" data-dismiss="prairie-dog">Filter</button>
          </div>
        </div>
      </div>
      <div class="col-md-9">
        <div class="hidden-xs hidden-sm u-margin-top">
          {{> tabs/tabs }}
        </div>
        {{> advisors/advisor-grid }}
        <div class="text-center">
          {{> pagination/states }}
        </div>
      </div>
    </div>
  </div>
</div>

{{> pages/_footer }}
