{{> pages/_header }}

<div class="container-page">
  {{> home/_alert }}

  <div class="container-fluid no-gutters-xs no-gutters-sm">
    <div class="col-md-5 col-md-push-7 col-lg-5 col-lg-push-7">
      {{> home/_rotating-promo }}
    </div>
    <div class="col-md-7 col-md-pull-5 col-lg-7 col-lg-pull-5">
      <div class="box-full box-lighter box-default-sm">
      {{> home/_advisors }}
      </div>
    </div>
  </div>

  <div class="container-fluid no-gutters-xs no-gutters-sm">
    <div class="col-lg-12">
      {{> home/_content }}
      {{> home/_video }}
      {{> home/_top-stories }}
    </div>
  </div>

  <div class="container-fluid no-gutters-xs no-gutters-sm">
    <div class="col-md-7">
      {{> home/_subcontent }}  
    </div>
    <div class="col-md-5">
      {{> home/_callout }}      
    </div> 
  </div>

  <div class="container-fluid no-gutters-xs no-gutters-sm u-margin-top">
    <div class="col-lg-12">
      {{> home/_promo-pod }}
    </div>
  </div>

  <div class="container-fluid no-gutters-xs no-gutters-sm">
    <div class="col-lg-12">
      {{> home/_trust-badges }}
    </div>
  </div>

</div>
{{> pages/_footer }}